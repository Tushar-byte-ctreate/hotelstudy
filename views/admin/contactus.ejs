<%- include('a-header'); -%>
<% if(error == "" || error == null ){%>

  <%}else{%>
    <span class="error">
      <p><%=error%></p> <i id="removeFlash" class="bi bi-x-lg"></i>
    </span>
  <%}%>
  <% if(info == "" ||info == null){%>
      
     
    <% }else{%>
      <span class="info">
          <p><%= info%> </p> <i   id="removeFlash" class="bi bi-x-lg"></i>
        </span>
      
  <%} %>
<div class="body-con" style='flex-direction:column;'>
    <div  class="user-wrapper">
        <h2>Contact</h2> Lists</h2>


        <% data.forEach(function(user){ %>
        <div class="user-card ">
           
          <a href=""><%=user.name%></a> <p><%=user.createdAt%></p> <span class=""><a href="/admin/contact/delete/<%=user._id%>"><i class="bi bi-trash-fill"></i></a>
            <a href=""><i class="bi bi-envelope"></i></a>
        </span>
        </div>
        <%})%>
        

    </div>
    </div>
</div>

<%- include('a-footer'); -%>
