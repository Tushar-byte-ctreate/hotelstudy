
<%- include('a-header'); -%>

<% if(error == "" || error == null){%>

  <% }else{ %>
  <span class="error">
    <p><%=error%></p> <i id="removeFlash" class="bi bi-x-lg"></i>
  </span>
<% } %>

<% if(info == "" ||info == null){%>

  <% }else{%>
  <span class="info">
    <p><%=info%> </p> <i id="removeFlash" class="bi bi-x-lg"></i>
  </span>
<%}%>
<div class="body-con" style="flex-direction: column">
  <div  class="user-wrapper">
    <h2>Change the Html</h2>
    <% data.forEach(function(data){%>
      <div class="user-card " style="background:#40348f;" onMouseOver="this.style.background='#ec639f'"  onMouseOut="this.style.background='#40348f'">
        <a href=""><%=data.name%></a>  <span class="">
          <a href="/admin/editheader/<%=data.value%>"><i class="bi bi-pencil"></i></a>
      </span>
      </div>
   <% })%>
    
    

</div>
</div>

<%- include('a-footer'); -%>